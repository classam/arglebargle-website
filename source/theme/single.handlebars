{{> partials/header }}

{{# each category_items }}
    <ul class='fpnl nav nav-pills nav-justified hidden-xs hidden-sm'>
        <li><a class='first' href="{{this.first.id}}.html">First</a></li>
        {{# if this.previous }}
            <li><a class='previous' href="{{this.previous.id}}.html">{{# if this.previous}} Previous {{/if}}</a></li>
        {{/if}}
        {{# if this.next }}
            <li><a class='next' href="{{this.next.id}}.html">Next</a></li>
        {{/if}}
        <li><a class='last' href="{{this.last.id}}.html">Last</a></li>
    </ul>
{{/each}}

<section class='post'>
    <h2>{{title}} - <small>{{human_datetime}}</small></h2>

    <div class='post_content'>
    {{{html}}}
    </div>

</section>

{{# each category_items }}
    <ul class='fpnl nav nav-pills nav-justified'>
        <li><a class='first' href="{{this.first.id}}.html">First</a></li>
        {{# if this.previous }}
            <li><a class='previous' href="{{this.previous.id}}.html">{{# if this.previous}} Previous {{/if}}</a></li>
        {{/if}}
        {{# if this.next }}
            <li><a class='next' href="{{this.next.id}}.html">Next</a></li>
        {{/if}}
        <li><a class='last' href="{{this.last.id}}.html">Last</a></li>
    </ul>
{{/each}}


<!-- DISQUS DROP-IN -->
